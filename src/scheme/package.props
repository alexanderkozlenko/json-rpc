<Project ToolsVersion="15.0">
  <PropertyGroup Condition="'$(Configuration)' == 'Release'">
    <DocumentationFile>$(OutputPath)$(TargetFramework)\$(AssemblyName).xml</DocumentationFile>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <RepositoryUrl>https://github.com/alexanderkozlenko/jsonrpcnet</RepositoryUrl>
    <PackageProjectUrl>$(RepositoryUrl)</PackageProjectUrl>
    <PackageLicenseUrl>$(RepositoryUrl)/LICENSE.md</PackageLicenseUrl>
    <PackageIconUrl>$(RepositoryUrl)/etc/img/nuget.png</PackageIconUrl>
    <Authors><![CDATA[JSON-RPC.NET Team]]></Authors>
    <PackageTags><![CDATA[json rpc]]></PackageTags>
  </PropertyGroup>
</Project>